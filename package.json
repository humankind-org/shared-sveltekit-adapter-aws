{
  "name": "@humankind/sveltekit-adapter-aws",
  "description": "SvelteKit to deploy your project using AWS CDK.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeBild/sveltekit-adapter-aws.git"
  },
  "bugs": {
    "url": "https://github.com/MikeBild/sveltekit-adapter-aws/issues"
  },
  "homepage": "https://github.com/MikeBild/sveltekit-adapter-aws#readme",
  "license": "MIT",
  "keywords": [
    "sveltekit",
    "adapter",
    "aws",
    "cdk"
  ],
  "author": "mike.bild@gmail.com",
  "version": "4.4.1",
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "destroy": "bin/destroy.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write .",
    "ca:login": "aws codeartifact login --tool npm --repository humankind-repository --domain humankind --namespace @humankind --duration-seconds 900 --profile npm --region us-east-1",
    "ca:logout": "npm config set registry https://registry.npmjs.com/",
    "npm:publish": "npm run build && npm run ca:login && npm publish && npm run ca:logout"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "2.6.2",
    "esbuild": "^0.17.5",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.63.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.63.0-alpha.0",
    "@sveltejs/kit": "^1.3.10",
    "aws-cdk": "^2.63.0",
    "aws-cdk-lib": "^2.63.0",
    "constructs": "^10.1.239",
    "dotenv": "16.0.3",
    "fs-extra": "^11.1.0",
    "svelte": "^3.55.1",
    "update-dotenv": "1.1.1",
    "vite": "^4.1.1"
  }
}
